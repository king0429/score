.wrap{
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.btnlist{
  width: 510rpx;
  margin: 0 auto;
  margin-top: 400rpx;
  font-size: 16px;
}
.btnlist view{
  width: 100%;
  background: #38adff;
  color: #fff;
  height: 80rpx;
  border: 1px solid #38beff;
  border-radius: 6rpx;
  margin: 50rpx 0;
  text-align: center;
  line-height: 80rpx;
}
